<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory), build.cmd))\tools\ServiceModel.Composition.props"/>
  <PropertyGroup>
    <NuSpecFile>ServiceModel.Composition.Registration.nuspec</NuSpecFile>
  </PropertyGroup>
  <ItemGroup>
    <NuGetContent Include="ServiceModel.Composition.Registration.dll;ServiceModel.Composition.Registration.pdb;ServiceModel.Composition.Registration.xml">
      <Source>$(OutputPath45)</Source>
      <Destination>lib\net45</Destination>
    </NuGetContent>
    <NuGetContent Include="ServiceModel.Composition.Registration.pdb" Condition="'$(NuGetPackSymbols)' == 'true'">
      <Source>$(OutputPath45)</Source>
      <Destination>lib\net45</Destination>
    </NuGetContent>
    <NuGetContentProject Include="$(RepositoryRoot)\src\ServiceModel.Composition.Registration\ServiceModel.Composition.Registration.csproj" Condition="'$(NuGetPackSymbols)' == 'true'" />
    <NuGetContent Include="tools\*">
      <Destination>tools</Destination>
    </NuGetContent>
    <NuGetContent Include="Content\*">
      <Destination>content</Destination>
    </NuGetContent>
  </ItemGroup>
  <Import Project="$(RepositoryRoot)Tools\NuGetProj.targets"/>
</Project>